# $Id: makefile.in,v 12.1 1997/09/12 01:33:01 tom Exp $

THIS	= ded

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Development) ##########################################################
USE	= man
SECTION	= 1
PREFIX	= $(INSTALL_MAN)/$(USE)$(SECTION)/

####### (Standard Lists) #######################################################
IT	=\
	$(PREFIX)$(THIS).$(SECTION)

SOURCES	=\
	$(THIS).man

ALL	=

####### (Standard Productions) #################################################
all:		$(SOURCES) $(ALL)
clean:					; -$(RM) $(CLEAN)
distclean:	clean			; -$(RM) $(ALL) *.cat
run_test:
sources:	$(SOURCES)
install:	$(IT)
deinstall:				; -$(RM) $(IT)

####### (Details of Productions) ###############################################
$(PREFIX)$(THIS).$(SECTION):	$(THIS).$(USE)	; $(PUT)
